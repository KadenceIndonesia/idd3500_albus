<!DOCTYPE html>
<html>
<head>
	<title>Dashboard - Festive <%= login[0].pid %></title>
    <link rel = "icon" href = "https://kadence.com/wp-content/uploads/2020/07/260x260.png" type = "image/x-icon">
	<meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0">
	<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
	<script src="<%= baseurl() %>plugin/flexmonster/theme/green/flexmonster.css"></script>
    <%- include ('./partials/header') %>
</head>
<style>
    .overview{
        padding: 10px 30px;
        background-color: #ffffff;
        border-radius: 8px;
    }
</style>
<body>
<div class="wrapper">
	<div class="lSide">
		<%- include ('./partials/sidebar') %>
	</div>
	<div class="rSide">
		<div class="head desk">
			<%- include ('./partials/topdesk') %>
		</div>
		<div class="mainbox size-std column">
			<div class="breadcrumbs size-std">
				DASHBOARD
			</div>
            <div class="overview full-length column shadow half">
                <div class="cols-3" style="color: #069E2D; border-right: 1px solid #f5f5f5; padding: 5px 2%;">
                    <p class="size-head3 nopad">Sales Booth <b style="float: right;">150(75<sup>%</sup>)</b></p>
                    <div class="progressbar">
                        <div class="progressresult" style="width: 75%; background-color: #069E2D;"></div>
                        <div class="progressdot" style="left: 74%; background-color: #069E2D;"></div>
                    </div>
                </div>
                <div class="cols-3" style="color: #058E3F; border-right: 1px solid #f5f5f5; padding: 5px 2%;">
                    <p class="size-head3 nopad">Service Point <b style="float: right;">75(37<sup>%</sup>)</b></p>
                    <div class="progressbar">
                        <div class="progressresult" style="width: 37%; background-color: #058E3F;"></div>
                        <div class="progressdot" style="left: 36%; background-color: #058E3F;"></div>
                    </div>
                </div>
                <div class="cols-3" style="color: #04773B; border-right: 1px solid #f5f5f5; padding: 5px 2%;">
                    <p class="size-head3 nopad">CEC <b style="float: right;">60(30<sup>%</sup>)</b></p>
                    <div class="progressbar">
                        <div class="progressresult" style="width: 30%; background-color: #04773B;"></div>
                        <div class="progressdot" style="left: 29%; background-color: #04773B;"></div>
                    </div>
                </div>
                <div class="cols-3" style="color: #036016; border-right: 1px solid #f5f5f5; padding: 5px 2%;">
                    <p class="size-head3 nopad">KYC Video <b style="float: right;">90(45<sup>%</sup>)</b></p>
                    <div class="progressbar">
                        <div class="progressresult" style="width: 45%; background-color: #036016;"></div>
                        <div class="progressdot" style="left: 44%; background-color: #036016;"></div>
                    </div>
                </div>
            </div>
			<div class="cols-12 containt" style="margin: 20px 0;">
				<div class="headtitle trans" style="margin-bottom: 0;">
					<p class="size-head3 nopad">Achievement By Skenario</p>
				</div>
				<div style="width: 100%;">
					<div class="chartbox">
						<div id="chartContainer" style="height: 370px; max-width: 100%; width: 100%; margin: 0px auto;"></div>
					</div>
				</div>
            </div>
		</div>
	</div>
</div>
</body>
<%- include ('./partials/footer') %>
<script>
window.onload = function () {	
	CanvasJS.addColorSet("flexmonsterStyle",
            [//colorSet Array
            "#069E2D",
            "#058E3F",
            "#04773B",
            "#036016",
    ]);
    var options = {
		title: {
			text: "Overall Performance (2017-2020)"              
		},
		colorSet: "flexmonsterStyle",
		exportEnabled: true,
		animationEnabled: true,
		axisY:{
			minimum: 0,
			maximum: 100,
		},
		axisX: {
			title: " ",
			labelFontSize: 16,
			labelWrap: true
		},
		data: [              
		{
			type: "column",
			indexLabel: "{y}",
			indexLabelFontSize: 18,
			dataPoints: [
				{ label: "Sales Booth ",  y: 75},
				{ label: "Service Point ", y: 37},
				{ label: "CEC ", y: 30},
				{ label: "KYC Video ", y: 45}
			]
		}
		]
	};
    $("#chartContainer").CanvasJSChart(options);
}
</script>
<script src="https://canvasjs.com/assets/script/jquery.canvasjs.min.js"></script>
</html>